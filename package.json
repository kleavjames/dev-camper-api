{
	"name": "devcamper_api",
	"version": "1.0.0",
	"description": "Devcamper backend API",
	"main": "server.js",
	"scripts": {
		"start": "SET NODE_ENV=production && SET PORT=5000 && node server",
		"dev": "nodemon -r dotenv/config server dotenv_config_path=./config/config.env",
		"import": "node -r dotenv/config seeder -import dotenv_config_path=./config/config.env",
		"destroy": "node -r dotenv/config seeder -destroy dotenv_config_path=./config/config.env"
	},
	"author": "Kleav James",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.1.7-alpha.3",
		"express-mongo-sanitize": "^2.0.0",
		"express-rate-limit": "^5.1.3",
		"helmet": "^3.22.0",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.9.13",
		"morgan": "^1.10.0",
		"node-geocoder": "^3.27.0",
		"nodemailer": "^6.4.6",
		"slugify": "^1.4.0",
		"xss-clean": "^0.1.1"
	},
	"devDependencies": {
		"nodemon": "^2.0.3"
	}
}
